#include<stdio.h>
#include<string.h>
int main()
{

    int i =0,cnt=0;
    char digit[106];
    scanf("%s",&digit);
    for(i=0;digit[i] != '\0';i++)
    {
        if(digit[i] == '1')
        {
            cnt = cnt+1;
            //printf("cnt = %d\n",cnt);
        }

    }

    if(cnt%2==0)
    {
      
    
    digit[i]='0';
    digit[i+1]='\0';
    printf("%s\n",digit);
    }
    else{
          int l;

    digit[i]='1';
    digit[i+1]='\0';
    printf("%s\n",digit);
    }





}
