#include<stdio.h>
int main()
{
    int test;
    scanf("%d",&test);
    while(test--)
    {

        int a,b,c;

        scanf("%d %d %d",&a,&b,&c);

        if(a+b==c)
        {
            printf("+\n");
        }
        else if(a-b==c)
        {
            printf("-\n");
        }

    }
    return 0;
}
